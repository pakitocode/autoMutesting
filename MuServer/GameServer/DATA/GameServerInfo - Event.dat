[GameServerInfo]
;==================================================
; Blood Castle Event Settings
;==================================================
; Enable Blood Castle Event (0 = No / 1 = Yes)
BloodCastleEvent=1

; Blood Castle Maximum Users per Castle
BloodCastleMaxUser=40

; Blood Castle enable PVP? (0 = No / 1 = Yes)
BloodCastleAllowPK=1

;==================================================
; Bonus Manager Settings
;==================================================
; Enable Bonus Manager Event (0 = No / 1 = Yes)
BonusManagerSwitch=1

;==================================================
; Devil Square Event Settings
;==================================================
; Enable Devil Square Event (0 = No / 1 = Yes)
DevilSquareEvent=1

; Devil Square Maximum Users per Square
DevilSquareMaxUser=100

; Devil Square enable PVP? (0 = No / 1 = Yes)
DevilSquareAllowPK=1

;==================================================
; Invasion Manager Settings
;==================================================
; Enable Invasions (0 = No / 1 = Yes)
InvasionManagerSwitch=1

; Enable Invasion Flying Dragons Effect (0 = No / 1 = Yes)
FlyingDragonsSwitch=1

; Dragons will only spawn in the map of the boss (0 = No / 1 = Yes)
FlyingDragonsOnlyBossMapSpawn=1

; Dragons will disappear when the boss dies (0 = No / 1 = Yes)
FlyingDragonsKillBossDisappear=1

;==================================================
; Golden Archer Settings
;==================================================
; Required Renas to exchange for ItemBag
; Use 0 to disable the button
