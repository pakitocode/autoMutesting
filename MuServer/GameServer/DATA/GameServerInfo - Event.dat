RenaExchangeRequired2=20
RenaExchangeRequired3=40
RenaExchangeRequired4=80

; Required Stones to exchange for VIP
; Use 0 to disable the button
; VipTime is in hours
StoneExchangeRequiredVIP1=100
StoneExchangeVIPTime1=1

StoneExchangeRequiredVIP2=250
StoneExchangeVIPTime2=1

StoneExchangeRequiredVIP3=500
StoneExchangeVIPTime3=1

; Required Stones to exchange for Lucky Number
; Use 0 to disable the button
StoneExchangeRequiredLuckyNumber=50

;==================================================
; Golden Archer Bingo Settings
;==================================================
; Enable Golden Archer Bingo Event (0 = No / 1 = Yes)
GoldenArcherBingoEvent=1

; Define the amount of winners
GoldenArcherWinnersAmount=1

; Delete all registered at the end of the event
GoldenArcherDeleteRegistered=1